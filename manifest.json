{
  "manifest_version": 3,
  "name": "Easy QR Code",
  "version": "1.2.0",
  "description": "Easy QR Code",
  "icons": {
    "16": "icon16x16.png",
    "24": "icon24x24.png",
    "48": "icon48x48.png",
    "64": "icon64x64.png",
    "128": "icon128x128.png"
  },
  "action": {
    "default_icon": "icon128x128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
